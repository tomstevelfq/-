/* pages/pt/pt.wxss */
page{
  background-color:rgba(226, 228, 228, 0.308);
  height:100%;
}

.swiper{
  height:400rpx
}

.imag1{
  width:320px;
  height:160px;
  display:inline-block;
  overflow:hidden;
}

.layout1{
  margin-right: 5%;
  margin-left: 5%;
  margin-top: 20rpx;
  display:flex;
  direction: row;
  justify-content: space-between;
}


.ar_line{
    width:80%;
    height:10rpx;
    content: '';
    background: #ddd;
    margin:30rpx 0rpx;
    margin-left: 10%;
}

.image {
  width: 250rpx;
  height: 150rpx;
  border-radius: 4px;
  margin-right: 100rpx;
  position: float;
  float: right;
}

.content {
  width: 150px;
  position: float;
  float: left;
  margin-left: 30rpx;
}

.text {
  height: 25px;
  color: rgba(10, 11, 11, 1);
  font-size: 14px;
  text-align: left;
  font-family: Roboto-regular;
}

.project {
  width: 100%;
  height: 150rpx;
  border-top: 5rpx;
  border-bottom: 5rpx;
  border-color: rgb(243, 243, 243);
  border-style: solid;
  border-width: 20rpx;
  padding: 30rpx;
  /* flex-direction: row; */
  margin: 0 auto;
}

.skipTop {
  position: fixed;
  bottom: 30rpx;
  right: 20rpx;
  width: 90rpx;
  height: 90rpx;
}

.skipTop image {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
